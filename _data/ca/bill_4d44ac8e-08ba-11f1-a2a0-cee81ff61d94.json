{"legislative_session": "20252026", "identifier": "SB916", "title": "Use of likeness: digital replica.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["Useoflikeness", "digitalreplica"], "abstracts": [{"note": "summary", "abstract": "Existing law makes a person who produces, distributes, or makes available the digital replica, as defined, of a deceased personality's voice or likeness in an expressive audiovisual work or sound recording without prior consent from a specified person liable to any injured party in an amount equal to the greater of $10,000 or the actual damages suffered by a person controlling the rights to the deceased personality's likeness, except as prescribed.\n\nThis bill would make a nonsubstantive change to that provision."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Introduced. Read first time. To Com. on RLS. for assignment. To print.", "date": "2026-01-27", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction", "reading-1"], "related_entities": []}, {"description": "From printer. May be acted upon on or after February 27.", "date": "2026-01-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Referred to Com. on RLS.", "date": "2026-02-11", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "Com. on RLS.", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Ashby", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Ashby\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "SB916", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260SB916", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "01/27/26 - Introduced",