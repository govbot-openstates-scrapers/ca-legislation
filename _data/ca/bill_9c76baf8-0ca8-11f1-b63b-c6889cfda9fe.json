{"legislative_session": "20252026", "identifier": "AB1913", "title": "Licensure: firefighting equipment.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Licensure", "firefightingequipment"], "abstracts": [{"note": "summary", "abstract": "Existing law establishes the practices and procedures for the issuance of a license to operate firefighting equipment as a driver or tiller operator. Existing law requires that the applicant either obtain and maintain a firefighter endorsement and either a class C license, restricted class A license, or noncommercial class B license, as specified, or obtain and maintain a class A or B license, as specified.\n\nThis bill would make technical, nonsubstantive changes to those provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-12", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "From printer. May be heard in committee March 15.", "date": "2026-02-13", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Soria", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Soria\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB1913", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB1913", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/12/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB1913&version=20250AB191399INT", "media_type": "application/pdf"}], "date": "2026-02-12", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB1913", "note": ""