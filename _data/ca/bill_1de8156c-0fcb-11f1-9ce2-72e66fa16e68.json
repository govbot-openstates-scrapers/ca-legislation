{"legislative_session": "20252026", "identifier": "AB2038", "title": "Insurance: crop insurance adjuster.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Insurance", "cropinsuranceadjuster"], "abstracts": [{"note": "summary", "abstract": "Existing law requires the Insurance Commissioner, upon application, to issue a crop insurance adjuster license to a person who has met specified requirements, including that they have satisfactorily completed the loss adjustment training curriculum and competency testing required by the Federal Crop Insurance Corporation Standard Reinsurance Agreement.\n\nThis bill would make technical, nonsubstantive changes to this provision."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-17", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "From printer. May be heard in committee March 20.", "date": "2026-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Harabedian", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Harabedian\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2038", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2038", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/17/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2038&version=20250AB203899INT", "media_type": "application/pdf"}], "date": "2026-02-17", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2038", "note": ""}], "extras": {"impact_clause": "An act to amen