{"legislative_session": "20252026", "identifier": "AB2740", "title": "Enforcement of money judgments: exemptions.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Enforcementofmoneyjudgments", "exemptions"], "abstracts": [{"note": "summary", "abstract": "Existing law identifies various types of property of a judgment debtor that are exempt from the enforcement of a money judgment, including material that in good faith is about to be applied to the repair or improvement of a residence, as specified.\n\nThis bill would specify that this exemption would apply to the judgment debtor's principal place of residence or domicile."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Macedo", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Macedo\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2740", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2740", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2740&version=20250AB274099INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2740", "note": ""}], "extras": {"impact_clause": "An act to amend Section 704.030 of the Code of Civil Procedure, relating to civil actions.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "name": "California", "classification": "state", "division_id": "ocd-division