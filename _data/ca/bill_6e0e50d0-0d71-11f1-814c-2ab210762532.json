{"legislative_session": "20252026", "identifier": "SB983", "title": "Insurance Commissioner: criminal violations.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["InsuranceCommissioner", "criminalviolations"], "abstracts": [{"note": "summary", "abstract": "Existing law provides for the powers and duties of the Insurance Commissioner. If the commissioner ascertains that a person has violated criminal provisions of the Insurance Code or other laws, existing law requires the commissioner to certify the facts of the violation to the district attorney of the county in which the offense was committed.\n\nThis bill would make technical, nonsubstantive changes to that provision."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Introduced. Read first time. To Com. on RLS. for assignment. To print.", "date": "2026-02-05", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction", "reading-1"], "related_entities": []}, {"description": "From printer. May be acted upon on or after March 8.", "date": "2026-02-09", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Referred to Com. on RLS.", "date": "2026-02-18", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "Com. on RLS.", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Weber Pierson", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Weber Pierson\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "SB983", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260SB983", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/05/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/b