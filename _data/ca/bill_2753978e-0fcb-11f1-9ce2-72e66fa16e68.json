{"legislative_session": "20252026", "identifier": "AB2688", "title": "Energy: offshore wind infrastructure.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Energy", "offshorewindinfrastructure"], "abstracts": [{"note": "summary", "abstract": "Existing law requires the State Energy Resources Conservation and Development Commission to establish and administer a program to support offshore wind infrastructure improvements to advance the capabilities of California ports, harbors, and other waterfront facilities to support the buildout of offshore wind facilities and maximize the economic and environmental benefits of an offshore wind industry in California.\n\nThis bill would make a nonsubstantive change to that requirement."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Zbur", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Zbur\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2688", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2688", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2688&version=20250AB268899INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2688", "note": ""}], "extras": {"impact_clause": "An act to amend Section 25666 of the Public Resources Code, relating to energy.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/gov