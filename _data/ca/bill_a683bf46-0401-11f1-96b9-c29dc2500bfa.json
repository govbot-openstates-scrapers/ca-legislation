{"legislative_session": "20252026", "identifier": "AB1499", "title": "Vehicles: accident reports.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Vehicles", "accidentreports"], "abstracts": [{"note": "summary", "abstract": "Existing law requires the driver of a motor vehicle who is involved in an accident that results in damage to the property of a person in excess of $1,000, or in death or bodily injury, to report the accident to the Department of Motor Vehicles within 10 days after the accident, as specified. A violation of these provisions is punishable as an infraction.\n\nThis bill would make technical, nonsubstantive changes to these provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Introduced. To print.", "date": "2025-02-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction"], "related_entities": []}, {"description": "From printer. May be heard in committee March 24.", "date": "2025-02-22", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Read first time.", "date": "2025-02-24", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Died at Desk.", "date": "2026-02-02", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Essayli", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Essayli\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB1499", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB1499", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/21/25 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB14