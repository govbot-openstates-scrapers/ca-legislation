{"legislative_session": "20252026", "identifier": "AB2695", "title": "Public Utilities Commission: reports.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["PublicUtilitiesCommission", "reports"], "abstracts": [{"note": "summary", "abstract": "Existing law vests the Public Utilities Commission with regulatory authority over public utilities. Existing law requires the commission to annually report to the Legislature on its timeliness in resolving cases, information on the disposition of applications for rehearings, and the days that its commissioners presided in and attended hearings, as provided.\n\nThis bill would require the commission to instead biannually make that report."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Gallagher", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Gallagher\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2695", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2695", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2695&version=20250AB269599INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2695", "note": ""}], "extras": {"impact_clause": "An act to amend Section 910.1 of the Public Utilities Code, relating to the Public Utilities Commission.", "tags": ["fiscal committee"]}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state