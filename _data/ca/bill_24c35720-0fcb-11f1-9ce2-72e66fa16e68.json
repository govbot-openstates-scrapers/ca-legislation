{"legislative_session": "20252026", "identifier": "AB2515", "title": "Childcare.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Childcare"], "abstracts": [{"note": "summary", "abstract": "Existing law, the Child Care and Development Services Act, administered by the State Department of Social Services, establishes a system of childcare and development services for children up to 13 years of age. Existing law requires the department to establish rules and regulations for the staffing of all center-based childcare and development programs under contract with the department.\n\nThis bill would make technical, nonsubstantive changes to that provision."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Harabedian", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Harabedian\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2515", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2515", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2515&version=20250AB251599INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2515", "note": ""}], "extras": {"impact_clause": "An act to amend Section 10275 of the Welfare and Institutions Code, relating to childcare.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "name": "California", "classification": "