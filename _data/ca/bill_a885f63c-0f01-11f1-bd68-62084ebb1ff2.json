{"legislative_session": "20252026", "identifier": "AB2585", "title": "Special education.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Specialeducation"], "abstracts": [{"note": "summary", "abstract": "Existing law requires that every individual with exceptional needs, as defined, who is eligible be provided with educational instruction, services, or both, at no cost to their parent or guardian or, as appropriate, to them. A free appropriate public education is required to be made available to individuals with exceptional needs in accordance with specified federal regulations adopted pursuant to the federal Individuals with Disabilities Education Act. \n\nThis bill would make nonsubstantive changes to those provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Arambula", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Arambula\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2585", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2585", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2585&version=20250AB258599INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2585", "note": ""}], "extras": {"impact_clause": "An act to amend Section 56040 of the Education Code, relating to special education.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/