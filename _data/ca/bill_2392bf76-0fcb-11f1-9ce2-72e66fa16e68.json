{"legislative_session": "20252026", "identifier": "AB2438", "title": "Imprisonment.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Imprisonment"], "abstracts": [{"note": "summary", "abstract": "Existing law requires a felony sentence punishable pursuant to specified provisions to be served in the county jail. Existing law requires these sentences to be served in the state prison if, among other things, the defendant has a current or prior serious or violent felony conviction or the defendant is required to register as a sex offender.\n\nThis bill would additionally require the sentence to be served in the state prison if the sentence results in a total term of imprisonment exceeding 6 years."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Johnson", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Johnson\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2438", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2438", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2438&version=20250AB243899INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2438", "note": ""}], "extras": {"impact_clause": "An act to amend Section 1170 of the Penal Code, relating to imprisonment.", "tags": ["fiscal committee"]}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "