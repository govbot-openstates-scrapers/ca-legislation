{"legislative_session": "20252026", "identifier": "SB952", "title": "Public lands: administration: impounded funds.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["Publiclands", "administration", "impoundedfunds"], "abstracts": [{"note": "summary", "abstract": "Existing law provides for the administration and control of state lands, including the leasing and sale of public lands, as provided. Existing law authorizes certain impounded funds to be invested and reinvested by the Director of Finance in bonds or other obligations of the United States, as provided.\n\nThis bill would make nonsubstantive changes to that provision to, among other things, use gender-neutral language."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Introduced. Read first time. To Com. on RLS. for assignment. To print.", "date": "2026-02-02", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction", "reading-1"], "related_entities": []}, {"description": "From printer. May be acted upon on or after March 5.", "date": "2026-02-03", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Referred to Com. on RLS.", "date": "2026-02-11", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "Com. on RLS.", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Laird", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Laird\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "SB952", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260SB952", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/02/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtm