{"legislative_session": "20252026", "identifier": "AB2722", "title": "Discharge: wage garnishment.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Discharge", "wagegarnishment"], "abstracts": [{"note": "summary", "abstract": "Existing law prohibits an employer from discharging an employee by reason of the fact that the garnishment of the employee's wages has been threatened or by reason of the fact that the employee's wages have been subjected to garnishment for the payment of one judgment and provides for the continuation of the wages of an employee who is discharged in violation of that provision, as prescribed.\n\nThis bill would make nonsubstantive changes to those provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Ellis", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Ellis\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2722", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2722", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2722&version=20250AB272299INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2722", "note": ""}], "extras": {"impact_clause": "An act to amend Section 2929 of the Labor Code, relating to employment.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "name": "California", "classification