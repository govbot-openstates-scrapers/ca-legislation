{"legislative_session": "20252026", "identifier": "SB1403", "title": "Automobile insurance.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["Automobileinsurance"], "abstracts": [{"note": "summary", "abstract": "Existing law generally regulates classes of insurance, including automobile insurance. Existing law prohibits an insurer that issues automobile collision or comprehensive automobile policies from refusing to issue a policy solely because of the age of the automobile to be insured, if the market value of that automobile exceeds $2,500.\n\nThis bill would make technical, nonsubstantive changes to that provision."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Introduced. To Com. on RLS. for assignment. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction"], "related_entities": []}], "sponsorships": [{"name": "Strickland", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Strickland\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "SB1403", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260SB1403", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260SB1403&version=20250SB140399INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260SB1403", "note": ""}], "extras": {"impact_clause": "An act to amend Section 671 of the Insurance Code, relating to insurance.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "name": "California", "classification": "state", "division_id