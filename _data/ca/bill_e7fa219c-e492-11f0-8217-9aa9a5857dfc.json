{"legislative_session": "20252026", "identifier": "AB1290", "title": "High-Speed Rail Authority: Senate confirmation.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["High-SpeedRailAuthority", "Senateconfirmation"], "abstracts": [{"note": "summary", "abstract": "Existing law creates the High-Speed Rail Authority with specified powers and duties relative to development and implementation of a high-speed train system. The authority is composed of 11 members, including 5 voting members appointed by the Governor, 4 voting members appointed by the Legislature, and 2 nonvoting legislative members.\n\nThis bill would require that the members of the authority appointed by the Governor be subject to appointment with the advice and consent of the Senate."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Introduced. To print.", "date": "2025-02-21", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["introduction"], "related_entities": []}, {"description": "From printer. May be heard in committee March 24.", "date": "2025-02-22", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Read first time.", "date": "2025-02-24", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Com. on TRANS.", "date": "2025-03-10", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": [{"name": "Com. on TRANS.", "entity_type": "organization", "organization_id": null}]}], "sponsorships": [{"name": "Wilson", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Wilson\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB1290", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=2025