{"legislative_session": "20252026", "identifier": "AB2642", "title": "Prisons: employment of inmates.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Prisons", "employmentofinmates"], "abstracts": [{"note": "summary", "abstract": "Existing law requires every able-bodied prisoner imprisoned in a state prison to work as many hours of faithful labor in each day and every day during their term of imprisonment as prescribed by the Secretary of the Department of Corrections and Rehabilitation in departmental rules and regulations.\n\nThis bill would make technical, nonsubstantive changes to that provision."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Elhawary", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Elhawary\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2642", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2642", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2642&version=20250AB264299INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2642", "note": ""}], "extras": {"impact_clause": "An act to amend Section 2700 of the Penal Code, relating to prisons.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "name": "California", "classification": "state", "division_id": "ocd-division/country:us/state:ca"}, "scraped_at": "