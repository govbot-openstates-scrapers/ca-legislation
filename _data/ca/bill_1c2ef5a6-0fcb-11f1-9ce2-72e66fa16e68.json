{"legislative_session": "20252026", "identifier": "AB1912", "title": "Deer: archery season: concealed firearms.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Deer", "archeryseason", "concealedfirearms"], "abstracts": [{"note": "summary", "abstract": "Existing law establishes an archery season for the taking of deer with bow and arrow. Existing law generally prohibits a person taking or attempting to take deer during that archery season from carrying, or having under their immediate control, a firearm of any kind, except for an active or honorably retired peace officer, as specified.\n\nThis bill would instead authorize any person to carry a firearm capable of being concealed on their person while engaging in taking or attempting to take a deer with a bow and arrow, as specified."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-12", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "From printer. May be heard in committee March 15.", "date": "2026-02-13", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Hadwick", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Hadwick\"}", "organization_id": null}, {"name": "Dahle", "classification": "principal coauthor", "entity_type": "person", "primary": false, "person_id": "~{\"name\": \"Dahle\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB1912", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB1912", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/12/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB1912&version=20250AB191299INT"