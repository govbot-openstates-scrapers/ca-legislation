{"legislative_session": "20252026", "identifier": "AB2148", "title": "School personnel.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Schoolpersonnel"], "abstracts": [{"note": "summary", "abstract": "Existing law establishes a system of public elementary and secondary education in this state. Under this system, school districts, county offices of education, and charter schools employ personnel to provide instruction to and services for pupils in kindergarten and grades 1 to 12, inclusive, at schoolsites throughout the state.\n\nThis bill would explicitly define, for purposes of certain provisions relating to the employment of school employees, an \"employee\" or \"person\" to mean a natural person."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "From printer. May be heard in committee March 21.", "date": "2026-02-19", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Muratsuchi", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Muratsuchi\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2148", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2148", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/18/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2148&version=20250AB214899INT", "media_type": "application/pdf"}], "date": "2026-02-18", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2148", "note": ""}], "ext