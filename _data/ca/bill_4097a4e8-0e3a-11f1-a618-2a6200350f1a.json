{"legislative_session": "20252026", "identifier": "AB2085", "title": "Weights and measures: point-of-sale systems.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Weightsandmeasures", "point-of-salesystems"], "abstracts": [{"note": "summary", "abstract": "Existing law requires the operator of a business establishment that uses a point-of-sale system to sell goods or services to consumers to ensure that the price of each good or service to be paid by the consumer is conspicuously displayed to the consumer at the time that the price is interpreted by the system, as provided. \n\nThis bill would make nonsubstantive changes to that requirement."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "From printer. May be heard in committee March 21.", "date": "2026-02-19", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Alanis", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Alanis\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2085", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2085", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/18/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2085&version=20250AB208599INT", "media_type": "application/pdf"}], "date": "2026-02-18", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2085", "note": ""}], "extras": {"impact_clause": "An act to amend Section 13300 of the Busines