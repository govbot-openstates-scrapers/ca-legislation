{"legislative_session": "20252026", "identifier": "AB2006", "title": "State agencies: privacy policy.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Stateagencies", "privacypolicy"], "abstracts": [{"note": "summary", "abstract": "Existing law requires each state department and state agency to enact and maintain a permanent privacy policy, in adherence with the Information Practices Act of 1977 and according to prescribed principles. Existing law requires each state department and state agency to conspicuously post, as defined, its privacy policy on its internet website.\n\nThis bill would make nonsubstantive changes to those provisions."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-17", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Michelle Rodriguez", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Michelle Rodriguez\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2006", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2006", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/17/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2006&version=20250AB200699INT", "media_type": "application/pdf"}], "date": "2026-02-17", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2006", "note": ""}], "extras": {"impact_clause": "An act to amend Section 11019.9 of the Government Code, relating to state government.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "name": "California", "classification": "