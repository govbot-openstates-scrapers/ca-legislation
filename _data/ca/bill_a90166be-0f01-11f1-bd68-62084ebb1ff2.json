{"legislative_session": "20252026", "identifier": "AB2623", "title": "State Air Resources Board.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["StateAirResourcesBoard"], "abstracts": [{"note": "summary", "abstract": "Existing law creates the State Air Resources Board as the state agency with the primary responsibility for the regulation of air pollution. Existing law authorizes the state board to do those acts as may be necessary for the proper execution of the powers and duties granted to, and imposed upon, the state board.\n\nThis bill would make nonsubstantive changes to that authorization."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Petrie-Norris", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Petrie-Norris\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2623", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2623", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2623&version=20250AB262399INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2623", "note": ""}], "extras": {"impact_clause": "An act to amend Section 39600 of the Health and Safety Code, relating to air pollution.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/government", "name": "California", "classification": "state", "division_id": "ocd-division/country:us/stat