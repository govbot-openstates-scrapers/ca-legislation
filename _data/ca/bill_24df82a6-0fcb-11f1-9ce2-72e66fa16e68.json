{"legislative_session": "20252026", "identifier": "AB2520", "title": "Business records: disclosure.", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": ["Businessrecords", "disclosure"], "abstracts": [{"note": "summary", "abstract": "Existing law prohibits a business entity that performs bookkeeping services from disclosing the contents of any record that is prepared or maintained by the business entity to any person, other than the individual or business entity that is the subject of the record, without express written consent, except as specified.\n\nThis bill would make nonsubstantive changes to the provision described above that prohibits business entities that perform bookkeeping services from making certain disclosures."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Read first time. To print.", "date": "2026-02-20", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}], "sponsorships": [{"name": "Krell", "classification": "author", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"Krell\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "AB2520", "links": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2520", "media_type": "text/html"}], "date": "", "classification": ""}, {"note": "02/20/26 - Introduced", "links": [{"url": "https://leginfo.legislature.ca.gov/faces/billPdf.xhtml?bill_id=202520260AB2520&version=20250AB252099INT", "media_type": "application/pdf"}], "date": "2026-02-20", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "http://leginfo.legislature.ca.gov/faces/billNavClient.xhtml?bill_id=202520260AB2520", "note": ""}], "extras": {"impact_clause": "An act to amend Section 1799.1 of the Civil Code, relating to civil law.", "tags": []}, "jurisdiction": {"id": "ocd-jurisdiction/country:us/state:ca/governmen